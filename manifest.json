{
  "name": "Comutiny",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Small extention description for Comutiny",
  "homepage_url": "http://example.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js" 
    ]
  },
  "options_page": "src/options_custom/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://twitter.com/*"
  ]
}